def normalizacion(X):
    n , m = X.shape
    medias = np.zeros(m)
    varianzas = np.zeros(m)
    for i in range(m):
        medias[i] = X[:,i].mean()
        varianzas[i] = X[:,i].std()
    for i in range(m):
        X[:,i] = (X[:,i] - medias[i])/varianzas[i]
    return X
    
